{
  "name": "covid-19-dwt-analysis",
  "private": true,
  "author": "Simon Hochholzer <simon@symmetronic.com>",
  "version": "0.5.0",
  "description": "Analysis of COVID-19 pandemic evolution using discrete wavelet transform.",
  "scripts": {
    "build": "stencil build",
    "generate": "stencil generate",
    "postinstall": "npm --prefix ./update install ./update",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:jekyll": "npm run build && jekyll serve --source www/covid-19-dwt-analysis --destination www/covid-19-dwt-analysis/_site --safe",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "update-data": "npm --prefix ./update start"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.3",
    "@stencil/sass": "^1.3.1",
    "@types/jest": "24.9.1",
    "@types/puppeteer": "2.0.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "puppeteer": "2.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/redux": "^0.2.0",
    "discrete-wavelets": "^5.0.2",
    "exact-linspace": "^1.0.3",
    "interp1": "^1.0.9",
    "lodash": "^4.17.19",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "strc-progress-bar": "^3.0.0",
    "tachyons-sass": "^4.9.5",
    "vega": "^5.10.1",
    "vega-embed": "^6.6.0",
    "vega-lite": "^4.10.4"
  }
}
